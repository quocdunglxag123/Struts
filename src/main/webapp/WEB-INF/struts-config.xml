<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
	<form-beans>
		<form-bean name="user" type="com.trungtamjava.form.loginForm" />
	</form-beans>
	
    <action-mappings>
        <action path="/T001" type="com.trungtamjava.action.UserAction"
        		name="user">
        	<forward name="search" path="/views/search.jsp"/>
        	<forward name="login" path="/views/login.jsp"/>
        </action>
    </action-mappings>
</struts-config>
